            layui.use('layer', function() {
                    layer = layui.layer;
                    $('#tag').click(function() {
                        $('#taglist').show();
                    })
                    $('.tag-switch').click(
                        function() {
                            if ($('#tag_' + $(this).data("id")).length > 0) {
                                $('#tag_' + $(this).data("id")).remove();
                            } else {
                                $("#tags").append(
                                    "<small id='tag_" + $(this).data("id")
                                    + "' class='ukefu-label "
                                    + $(this).data("theme") + "'>"
                                    + $(this).data("name") + "</small>");
                            }
                            loadURL($(this).data("href"));
                        });
                    $("#taglist").hover(null, function() {
                        $(this).hide();
                    });
